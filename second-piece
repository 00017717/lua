--UI Setup
--GetUILib
local Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = Lib:Load("Takai's Library | Second Piece", "Default")
--//--
--Tabs Setup
local home_tab = Lib.newTab("Home", "ImageIdHere")
local teleport_tab = Lib.newTab("Teleport", "ImageIdHere")
--//--

--HomeTab Functions

--//--
--TeleportTab Functions
teleport_tab.newButton("TP To Starter Island", "Teleport to Starter Island's Spawn [LV.0+]", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-894, 35, -567)
end)

